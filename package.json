{
    "name": "discord-cmd-parser",
    "version": "1.1.2",
    "description": "Parser for chat commands",
    "main": "dist/index.js",
    "author": "RynerNO",
    "license": "MIT",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/RynerNO/discord-cmd-parser.git",
    "keywords": [
        "discord",
        "discordjs",
        "args",
        "parser",
        "commands"
    ],
    "scripts": {
