        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "jest": "^26.4.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "ts-jest": "^26.2.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7",
        "uglify-js": "^3.10.1",
